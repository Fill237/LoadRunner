<script type="text/template" id="eventEntryTmpl">
<div class="swiper-wrapper">
  {{~it.products: value: index}}
  <div class="swiper-slide {{? index == 0 }} move-fast {{?? index == 1}} move-slow{{?}} " data-index="{{=index + 1}}">
    <div class="left-block">
      <img class="event-product"  src="{{=value.productImg}}?width=360&height=360" alt="" >
    </div>
    <div class="right-block">
      <div class="right-product-info" style="color: {{=it.textColor}}">
        <h4 class="event-product-name " >{{=value.productTitle}}</h4>
        {{? value.productDesc }}
        <div class="parameter-box">
          <div class="parameter" style="color: {{=it.textColor}}">{{=value.productDesc}}</div>
        </div>
        {{?}}
      </div>
      <div class="origin-price" style="color: {{=it.textColor}}" >
      {{? value.originPrice && it.isCurrencyPre }}
        {{=it.currency}}
        {{=value.originPrice}}
      {{?? value.originPrice}}
        {{=value.originPrice}}
        {{=it.currency}}
      {{?}}
      </div>
      <div class="market-price"  style="color: {{=it.textColor}}">
      {{? value.currentPrice && it.isCurrencyPre }}
        {{=it.currency}}
        {{=value.currentPrice}}
      {{?? value.currentPrice}}

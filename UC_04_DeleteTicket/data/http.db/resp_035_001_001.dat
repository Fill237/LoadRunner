<script id="flashSaleEvtTempl" type="text/template">
{{~it: value}}
  <li class="home-flash-list__item" data-act-id="{{=value.id}}" data-begin="{{=value.start}}" data-end="{{=value.end}}">
    <div class="home-flash-list__header">
      <div class="home-flash-list__day">{{=value.day}}</div>
      <div class="home-flash-list__time">
        <strong></strong>
        <div class="home-flash-list__countdown"></div>
      </div>
    </div>
    <ul class="home-flash-list__content">
      {{~value.products: product}}
        <li class="home-flash-list__product" data-act-id="{{=value.id}}" data-goods-id="{{=product.goods_id}}" data-product-url="{{=product.buy_url}}">
          <div class="home-flash-list__product-info">
            <h3 class="home-flash-list__product-title">{{=product.name}}</h3>
            <ul class="home-flash-list__product-tags">
              {{~product.tags: tag}}
              <li class="home-flash-list__product-tag">
                <span>{{=tag.name}}</span>
                {{? tag.type == 3 && tag.detail}}
                  <i>{{=tag.detail}}</i>
                {{?}}
              </li>
              {{~}}
            </ul>
          </div>
          <img class="home-flash-list__product-image home-flash-list__product-image--regular" src="{{=product.img_url}}" alt="" />
          <img class="home-flash-list__product-image home-flash-list__product-image--promo"   src="{{=product.pre_url}}" alt="" />
          <div class="home-flash-list__product-price">
            <strong>
              {{=product.new_price}} ₽            </strong>
            {{? product.old_price && product.old_price !== product.new_price}}
              <del>
                {{=product.old_price}} ₽              </del>
            {{?}}
          </div>
          {{? product.gift_url}}
          <div class="home-flash-list__product-gift">
            <img src="{{=product.gift_url}}" alt="" />
            <span>Подарок</span>
          </div>
          {{?}}
        </li>
      {{~}}
    </ul>
    <a class="btn btn-see-more"></a>
  </li>
{{~}}
</script>
